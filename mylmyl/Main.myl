module Main;

import Parser;
import Compiler;
import Typechecker;

fun main() do 
    val source := openFile("input.myl");
    if isNull(source) do 
        panic("Could not open file");
    end;
    val ast := Parser.parse(source);
    Typechecker.typecheck(ast);
    val compiled := Compiler.compile(ast);
    print(compiled);
end

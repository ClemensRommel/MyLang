fun A(k : Number, x1 : Fun() : Number, x2 : Fun() : Number, x3 : Fun() : Number, x4 : Fun() : Number, x5 : Fun() : Number) : Number do
    var m : Number := k;
    fun B() : Number do
        m := m - 1;
        A(m, B, x1, x2, x3, x4)
    end

    if k <= 0 then x4() + x5() else B()
end

fun main() : Void do
    print A(10, fun() : Number := 1, fun() : Number := -1, fun() : Number := -1, fun() : Number := 1, fun() : Number := 0);
end
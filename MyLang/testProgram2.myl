fun fibbonacciN(n: Number): Number {
    var last: Number := 1;
    var lastBefore: Number := 1;
    for i in [2..n] do {
        val lastBefore2: Number := lastBefore;
        lastBefore := last;
        last := last + lastBefore2;
    };
    last
};

/* druckt /* die fibbonacci */ zahlen */
fun main() {
    print(fibbonacciN(40));
};
